{
  "name": "please-prototype",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "please": "./please"
  },
  "scripts": {
    "0": "node ./dist/language/cli/0.js",
    "1": "node ./dist/language/cli/1.js",
    "2": "node ./dist/language/cli/2.js",
